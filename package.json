		"watch:sass": "npx sass --watch src/scss:public/css --load-path=src/scss",
		"watch": "npm-run-all --parallel watch:sass dev",
		"clean": "rm -rf public && npm run build",
		"wipe": "rm -rf node_modules && rm -rf package-lock.json",
